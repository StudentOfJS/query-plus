{
  "name": "query-plus",
  "author": "Rod Lewis",
  "description": "Fetch data from a remote API with React",
  "version": "1.0.7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StudentOfJS/query-plus.git"
  },
  "keywords": [
    "typescript",
    "react",
    "indexeddb",
    "web-worker",
    "worker",
    "fetch"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies;\"",
    "test": "vitest --config ./vitest.config.ts",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/queryPlus.umd.js",
  "module": "./dist/queryPlus.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/queryPlus.es.js",
      "require": "./dist/queryPlus.umd.js"
    }
  },
  "dependencies": {
    "idb-keyval": "^6.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@vitejs/plugin-react": "^2.1.0",
    "happy-dom": "^7.6.0",
    "json": "^11.0.0",
    "react": "^18.2.0",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.2.0",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "idb-keyval": "^6.2.0",
    "react": ">=16.8.0"
  }
}
