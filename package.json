{
  "name": "react-fetch",
  "description": "Fetch data from a remote API with React",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "test": "vitest --config ./vitest.config.ts",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/react-fetch.umd.js",
  "module": "./dist/react-fetch.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-fetch.es.js",
      "require": "./dist/react-fetch.umd.js"
    }
  },
  "dependencies": {
    "idb-keyval": "^6.2.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "happy-dom": "^5.3.1",
    "json": "^11.0.0",
    "vite-plugin-dts": "^1.2.0",
    "vitest": "^0.15.2"
  }
}
